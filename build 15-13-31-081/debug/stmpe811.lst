 208:Drivers/BSP/Components/stmpe811/stmpe811.c ****   IOE_Write(DeviceAddr, STMPE811_REG_INT_CTRL, tmp); 
 209:Drivers/BSP/Components/stmpe811/stmpe811.c **** }
