 129:Drivers/BSP/Components/stmpe811/stmpe811.c **** void stmpe811_Init(uint16_t DeviceAddr)
 130:Drivers/BSP/Components/stmpe811/stmpe811.c **** {
 131:Drivers/BSP/Components/stmpe811/stmpe811.c ****   uint8_t instance;
 132:Drivers/BSP/Components/stmpe811/stmpe811.c ****   uint8_t empty;
 133:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 134:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Check if device instance already exists */
 135:Drivers/BSP/Components/stmpe811/stmpe811.c ****   instance = stmpe811_GetInstance(DeviceAddr);
 136:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 137:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* To prevent double initialization */
 138:Drivers/BSP/Components/stmpe811/stmpe811.c ****   if(instance == 0xFF)
 139:Drivers/BSP/Components/stmpe811/stmpe811.c ****   {
 140:Drivers/BSP/Components/stmpe811/stmpe811.c ****     /* Look for empty instance */
 141:Drivers/BSP/Components/stmpe811/stmpe811.c ****     empty = stmpe811_GetInstance(0);
 142:Drivers/BSP/Components/stmpe811/stmpe811.c ****     
 143:Drivers/BSP/Components/stmpe811/stmpe811.c ****     if(empty < STMPE811_MAX_INSTANCE)
 144:Drivers/BSP/Components/stmpe811/stmpe811.c ****     {
 145:Drivers/BSP/Components/stmpe811/stmpe811.c ****       /* Register the current device instance */
