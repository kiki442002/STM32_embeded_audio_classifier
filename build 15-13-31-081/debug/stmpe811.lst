 201:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Read the Interrupt Control register  */
 202:Drivers/BSP/Components/stmpe811/stmpe811.c ****   tmp = IOE_Read(DeviceAddr, STMPE811_REG_INT_CTRL);
ARM GAS  /var/folders/n6/c271nzx11xl4r4_m7sqjlt0m0000gn/T//cc2SfOSY.s 			page 5


 203:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
 204:Drivers/BSP/Components/stmpe811/stmpe811.c ****   /* Set the global interrupts to be Enabled */    
 205:Drivers/BSP/Components/stmpe811/stmpe811.c ****   tmp |= (uint8_t)STMPE811_GIT_EN;
 206:Drivers/BSP/Components/stmpe811/stmpe811.c ****   
